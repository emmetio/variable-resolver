{
  "name": "@emmetio/variable-resolver",
  "version": "0.1.0",
  "description": "Resolves ${variables} in given parsed abbreviation tree",
  "main": "dist/variable-resolver.cjs.js",
  "jsnext:main": "dist/variable-resolver.es.js",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -o dist/variable-resolver.cjs.js -f cjs ./index.js",
    "build:es": "rollup -o dist/variable-resolver.es.js -f es ./index.js",
    "prepublish": "npm run test && npm run build",
    "watch": "rollup -w -o dist/variable-resolver.cjs.js -f cjs ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmetio/variable-resolver.git"
  },
  "keywords": [
    "emmet",
    "variable",
    "parser"
  ],
  "author": "Sergey Chikuyonok <serge.che@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emmetio/variable-resolver/issues"
  },
  "homepage": "https://github.com/emmetio/variable-resolver#readme",
  "devDependencies": {
    "@emmetio/abbreviation": "^0.4.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-register": "^6.18.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-watch": "^2.5.0"
  }
}
